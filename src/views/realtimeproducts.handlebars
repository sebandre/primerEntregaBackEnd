<div>
    <h1>Lista de productos</h1>
    <ul>
        {{#each products}}
        <li>
            <h3>{{title}}</h3>
            <p>{{description}}</p>
            <p>Precio: {{price}}</p>
            {{!-- <p>Thumbnail: <img src="{{thumbnail}}" alt="Thumbnail"></p> --}}
            <p>CÃ³digo: {{code}}</p>
            <p>Stock: {{stock}}</p>
            <hr>
        </li>
        {{/each}}
    </ul>
</div>
<div>
<form id= "formSocket">
  <label class="title">Title:</label>
  <input type="text" id="title" name="title"><br>

  <label class="description">Description:</label>
  <textarea id="description" name="description"></textarea><br>

  <label class="price">Price:</label>
  <input type="number" id="price" name="price"><br>

  <label class="status">Status:</label>
  <input type="boolean" id="status" name="status"><br>

  <label class="thumbnail">Thumbnail:</label>
  <input type="text" id="thumbnail" name="thumbnail"><br>

  <label class="code">Code:</label>
  <input type="text" id="code" name="code"><br>

  <label class="stock">Stock:</label>
  <input type="number" id="stock" name="stock"><br>

  <input type="submit" value="Agregar producto">
</form>
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>

